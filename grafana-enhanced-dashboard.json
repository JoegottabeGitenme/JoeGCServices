{
  "uid": "wms-perf",
  "title": "WMS Performance - Complete Pipeline Analysis",
  "tags": ["wms", "performance", "profiling"],
  "timezone": "browser",
  "refresh": "5s",
  "schemaVersion": 40,
  "version": 2,
  "panels": [
    {
      "datasource": {"type": "prometheus"},
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {"drawStyle": "line", "fillOpacity": 10}
        }
      },
      "gridPos": {"h": 6, "w": 8, "x": 0, "y": 0},
      "id": 1,
      "targets": [
        {
          "expr": "rate(wms_requests_total[1m])",
          "legendFormat": "WMS",
          "refId": "A"
        },
        {
          "expr": "rate(wmts_requests_total[1m])",
          "legendFormat": "WMTS",
          "refId": "B"
        },
        {
          "expr": "rate(renders_total[1m])",
          "legendFormat": "Renders",
          "refId": "C"
        }
      ],
      "title": "Requests/sec (tiles_per_second)",
      "type": "timeseries"
    },
    {
      "datasource": {"type": "prometheus"},
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "custom": {"drawStyle": "line", "fillOpacity": 20}
        }
      },
      "gridPos": {"h": 6, "w": 8, "x": 8, "y": 0},
      "id": 2,
      "targets": [
        {
          "expr": "rate(cache_hits_total[1m]) / (rate(cache_hits_total[1m]) + rate(cache_misses_total[1m])) * 100",
          "legendFormat": "Cache Hit Rate",
          "refId": "A"
        }
      ],
      "title": "Cache Hit Rate (%)",
      "type": "timeseries"
    },
    {
      "datasource": {"type": "prometheus"},
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {"mode": "thresholds"},
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "red", "value": 1}
            ]
          }
        }
      },
      "gridPos": {"h": 6, "w": 8, "x": 16, "y": 0},
      "id": 3,
      "targets": [
        {
          "expr": "renders_total - sum(increase(renders_total[1m]))",
          "legendFormat": "Errors",
          "refId": "A"
        }
      ],
      "title": "Render Errors",
      "type": "stat",
      "options": {"colorMode": "background"}
    },
    {
      "datasource": {"type": "prometheus"},
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {"drawStyle": "line", "fillOpacity": 10, "stacking": {"mode": "normal"}}
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 6},
      "id": 4,
      "targets": [
        {
          "expr": "grib_load_duration_ms{quantile=\"0.5\"}",
          "legendFormat": "GRIB Load",
          "refId": "A"
        },
        {
          "expr": "grib_parse_duration_ms{quantile=\"0.5\"}",
          "legendFormat": "GRIB Parse",
          "refId": "B"
        },
        {
          "expr": "resample_duration_ms{quantile=\"0.5\"}",
          "legendFormat": "Resample",
          "refId": "C"
        },
        {
          "expr": "png_encode_duration_ms{quantile=\"0.5\"}",
          "legendFormat": "PNG Encode",
          "refId": "D"
        }
      ],
      "title": "Pipeline Stages - p50 Latency (Stacked)",
      "type": "timeseries"
    },
    {
      "datasource": {"type": "prometheus"},
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {"drawStyle": "line", "fillOpacity": 10}
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 6},
      "id": 5,
      "targets": [
        {
          "expr": "render_duration_ms{quantile=\"0.5\"}",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "render_duration_ms{quantile=\"0.9\"}",
          "legendFormat": "p90",
          "refId": "B"
        },
        {
          "expr": "render_duration_ms{quantile=\"0.95\"}",
          "legendFormat": "p95",
          "refId": "C"
        },
        {
          "expr": "render_duration_ms{quantile=\"0.99\"}",
          "legendFormat": "p99",
          "refId": "D"
        }
      ],
      "title": "Total Render Latency - Percentiles",
      "type": "timeseries"
    },
    {
      "datasource": {"type": "prometheus"},
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      },
      "gridPos": {"h": 4, "w": 4, "x": 0, "y": 14},
      "id": 6,
      "targets": [
        {
          "expr": "renders_total",
          "refId": "A"
        }
      ],
      "title": "Total Renders",
      "type": "stat",
      "options": {"colorMode": "value", "graphMode": "area"}
    },
    {
      "datasource": {"type": "prometheus"},
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 10},
              {"color": "red", "value": 100}
            ]
          }
        }
      },
      "gridPos": {"h": 4, "w": 4, "x": 4, "y": 14},
      "id": 7,
      "targets": [
        {
          "expr": "render_duration_ms{quantile=\"0.5\"}",
          "refId": "A"
        }
      ],
      "title": "Render Time p50",
      "type": "stat",
      "options": {"colorMode": "background", "graphMode": "area"}
    },
    {
      "datasource": {"type": "prometheus"},
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 50},
              {"color": "red", "value": 500}
            ]
          }
        }
      },
      "gridPos": {"h": 4, "w": 4, "x": 8, "y": 14},
      "id": 8,
      "targets": [
        {
          "expr": "render_duration_ms{quantile=\"0.99\"}",
          "refId": "A"
        }
      ],
      "title": "Render Time p99",
      "type": "stat",
      "options": {"colorMode": "background", "graphMode": "area"}
    },
    {
      "datasource": {"type": "prometheus"},
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      },
      "gridPos": {"h": 4, "w": 4, "x": 12, "y": 14},
      "id": 9,
      "targets": [
        {
          "expr": "cache_hits_total",
          "refId": "A"
        }
      ],
      "title": "Cache Hits",
      "type": "stat",
      "options": {"colorMode": "value", "graphMode": "area"}
    },
    {
      "datasource": {"type": "prometheus"},
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {"mode": "thresholds"},
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 100},
              {"color": "red", "value": 1000}
            ]
          }
        }
      },
      "gridPos": {"h": 4, "w": 4, "x": 16, "y": 14},
      "id": 10,
      "targets": [
        {
          "expr": "cache_misses_total",
          "refId": "A"
        }
      ],
      "title": "Cache Misses",
      "type": "stat",
      "options": {"colorMode": "background", "graphMode": "area"}
    },
    {
      "datasource": {"type": "prometheus"},
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "max": 100,
          "min": 0
        }
      },
      "gridPos": {"h": 4, "w": 4, "x": 20, "y": 14},
      "id": 11,
      "targets": [
        {
          "expr": "rate(cache_hits_total[5m]) / (rate(cache_hits_total[5m]) + rate(cache_misses_total[5m])) * 100",
          "refId": "A"
        }
      ],
      "title": "Cache Hit Rate (5m avg)",
      "type": "stat",
      "options": {"colorMode": "value", "graphMode": "none"}
    },
    {
      "datasource": {"type": "prometheus"},
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 18},
      "id": 12,
      "targets": [
        {
          "expr": "grib_load_duration_ms_sum / render_duration_ms_sum * 100",
          "legendFormat": "GRIB Load",
          "refId": "A"
        },
        {
          "expr": "grib_parse_duration_ms_sum / render_duration_ms_sum * 100",
          "legendFormat": "GRIB Parse",
          "refId": "B"
        },
        {
          "expr": "resample_duration_ms_sum / render_duration_ms_sum * 100",
          "legendFormat": "Resample",
          "refId": "C"
        },
        {
          "expr": "png_encode_duration_ms_sum / render_duration_ms_sum * 100",
          "legendFormat": "PNG Encode",
          "refId": "D"
        }
      ],
      "title": "Pipeline Breakdown (% of Total Time)",
      "type": "piechart",
      "options": {
        "legend": {"displayMode": "table", "placement": "right", "values": ["value", "percent"]}
      }
    },
    {
      "datasource": {"type": "prometheus"},
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {"drawStyle": "line", "fillOpacity": 10}
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 18},
      "id": 13,
      "targets": [
        {
          "expr": "grib_load_duration_ms{quantile=\"0.99\"}",
          "legendFormat": "GRIB Load p99",
          "refId": "A"
        },
        {
          "expr": "grib_parse_duration_ms{quantile=\"0.99\"}",
          "legendFormat": "GRIB Parse p99",
          "refId": "B"
        },
        {
          "expr": "resample_duration_ms{quantile=\"0.99\"}",
          "legendFormat": "Resample p99",
          "refId": "C"
        },
        {
          "expr": "png_encode_duration_ms{quantile=\"0.99\"}",
          "legendFormat": "PNG Encode p99",
          "refId": "D"
        }
      ],
      "title": "Pipeline Stages - p99 Latency (Worst Case)",
      "type": "timeseries"
    }
  ]
}
