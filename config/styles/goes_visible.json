{
  "version": "1.0",
  "metadata": {
    "name": "GOES Visible Styles",
    "description": "Grayscale colormap for GOES visible band reflectance"
  },
  "styles": {
    "default": {
      "name": "GOES Visible",
      "description": "Grayscale colormap for GOES visible band reflectance",
      "type": "gradient",
      "units": "reflectance",
      "range": {
        "min": 0.0,
        "max": 1.0
      },
      "stops": [
        { "value": 0.0, "color": "#000000", "label": "0%" },
        { "value": 0.1, "color": "#191919", "label": "10%" },
        { "value": 0.2, "color": "#333333", "label": "20%" },
        { "value": 0.3, "color": "#4C4C4C", "label": "30%" },
        { "value": 0.4, "color": "#666666", "label": "40%" },
        { "value": 0.5, "color": "#7F7F7F", "label": "50%" },
        { "value": 0.6, "color": "#999999", "label": "60%" },
        { "value": 0.7, "color": "#B2B2B2", "label": "70%" },
        { "value": 0.8, "color": "#CCCCCC", "label": "80%" },
        { "value": 0.9, "color": "#E5E5E5", "label": "90%" },
        { "value": 1.0, "color": "#FFFFFF", "label": "100%" }
      ],
      "interpolation": "linear",
      "out_of_range": "clamp",
      "legend": {
        "title": "Reflectance",
        "labels": ["0%", "25%", "50%", "75%", "100%"]
      }
    }
  }
}
