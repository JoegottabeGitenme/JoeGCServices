{
  "version": "1.0",
  "metadata": {
    "name": "Cloud Cover Styles",
    "description": "Styles for cloud cover visualization"
  },
  "styles": {
    "default": {
      "default": true,
      "name": "Cloud Cover",
      "description": "Cloud cover percentage from clear to overcast",
      "type": "gradient",
      "units": "%",
      "range": {
        "min": 0,
        "max": 100
      },
      "stops": [
        { "value": 0, "color": "#FFFFFF00", "label": "Clear" },
        { "value": 10, "color": "#F5F5F5", "label": "10%" },
        { "value": 25, "color": "#E0E0E0", "label": "25%" },
        { "value": 40, "color": "#C0C0C0", "label": "40%" },
        { "value": 50, "color": "#A0A0A0", "label": "50%" },
        { "value": 60, "color": "#808080", "label": "60%" },
        { "value": 75, "color": "#606060", "label": "75%" },
        { "value": 90, "color": "#404040", "label": "90%" },
        { "value": 100, "color": "#303030", "label": "Overcast" }
      ],
      "interpolation": "linear",
      "out_of_range": "clamp",
      "legend": {
        "title": "Cloud Cover (%)",
        "labels": ["Clear", "25%", "50%", "75%", "Overcast"]
      }
    }
  }
}
