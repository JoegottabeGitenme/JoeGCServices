      "gridPos": {"h": 7, "w": 8, "x": 16, "y": 9},
      "id": 6,
      "targets": [
        {
          "expr": "sum(rate(data_source_cache_hits_total{source=\"mrms\"}[$__rate_interval])) / (sum(rate(data_source_cache_hits_total{source=\"mrms\"}[$__rate_interval])) + sum(rate(data_source_parse_total{source=\"mrms\"}[$__rate_interval]))) * 100",
