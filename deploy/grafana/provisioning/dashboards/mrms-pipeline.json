{
  "uid": "mrms-pipeline",
  "title": "MRMS Radar Pipeline - Complete Analysis",
  "tags": [
    "mrms",
    "radar",
    "grib2",
    "observation",
    "performance"
  ],
  "timezone": "browser",
  "refresh": "5s",
  "schemaVersion": 40,
  "version": 1,
  "panels": [
    {
      "type": "row",
      "title": "Overview - MRMS Request & Throughput",
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 0},
      "collapsed": false
    },
    {
      "datasource": {"type": "prometheus"},
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 20,
            "gradientMode": "opacity"
          },
          "color": {"mode": "palette-classic"}
        }
      },
      "gridPos": {"h": 7, "w": 8, "x": 0, "y": 1},
      "id": 1,
      "targets": [
        {
          "expr": "sum(rate(model_requests_total{model=\"mrms\"}[$__rate_interval]))",
          "legendFormat": "Total Requests",
          "refId": "A"
        },
        {
          "expr": "sum(rate(model_requests_total{model=\"mrms\"}[$__rate_interval])) by (parameter)",
          "legendFormat": "{{parameter}}",
          "refId": "B"
        }
      ],
      "title": "MRMS Requests/sec",
      "type": "timeseries"
    },
    {
      "datasource": {"type": "prometheus"},
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 20
          }
        }
      },
      "gridPos": {"h": 7, "w": 8, "x": 8, "y": 1},
      "id": 2,
      "targets": [
        {
          "expr": "sum(rate(model_renders_total{model=\"mrms\", success=\"true\"}[$__rate_interval]))",
          "legendFormat": "Successful",
          "refId": "A"
        },
        {
          "expr": "sum(rate(model_renders_total{model=\"mrms\", success=\"false\"}[$__rate_interval]))",
          "legendFormat": "Failed",
          "refId": "B"
        }
      ],
      "title": "MRMS Renders/sec (Success vs Failed)",
      "type": "timeseries"
    },
    {
      "datasource": {"type": "prometheus"},
      "fieldConfig": {
        "defaults": {
          "unit": "Bps",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 30,
            "gradientMode": "opacity"
          }
        }
      },
      "gridPos": {"h": 7, "w": 8, "x": 16, "y": 1},
      "id": 3,
      "targets": [
        {
          "expr": "sum(rate(model_fetch_bytes_total{model=\"mrms\"}[$__rate_interval]))",
          "legendFormat": "Data Throughput",
          "refId": "A"
        }
      ],
      "title": "MRMS Data Throughput (bytes/sec)",
      "type": "timeseries"
    },
    {
      "type": "row",
      "title": "Cache Performance",
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 8},
      "collapsed": false
    },
    {
      "datasource": {"type": "prometheus"},
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 20
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "red", "value": null},
              {"color": "yellow", "value": 50},
              {"color": "green", "value": 80}
            ]
          }
        }
      },
      "gridPos": {"h": 7, "w": 8, "x": 0, "y": 9},
      "id": 4,
      "targets": [
        {
          "expr": "sum(rate(model_cache_hits_total{model=\"mrms\"}[$__rate_interval])) / (sum(rate(model_cache_hits_total{model=\"mrms\"}[$__rate_interval])) + sum(rate(model_cache_misses_total{model=\"mrms\"}[$__rate_interval]))) * 100",
          "legendFormat": "GRIB File Cache",
          "refId": "A"
        },
        {
          "expr": "sum(rate(model_grid_cache_hits_total{model=\"mrms\"}[$__rate_interval])) / (sum(rate(model_grid_cache_hits_total{model=\"mrms\"}[$__rate_interval])) + sum(rate(model_grid_cache_misses_total{model=\"mrms\"}[$__rate_interval]))) * 100",
          "legendFormat": "Parsed Grid Cache",
          "refId": "B"
        }
      ],
      "title": "MRMS Cache Hit Rate",
      "type": "timeseries"
    },
    {
      "datasource": {"type": "prometheus"},
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        }
      },
      "gridPos": {"h": 7, "w": 8, "x": 8, "y": 9},
      "id": 5,
      "targets": [
        {
          "expr": "sum(rate(model_cache_hits_total{model=\"mrms\"}[$__rate_interval]))",
          "legendFormat": "GRIB Hits",
          "refId": "A"
        },
        {
          "expr": "sum(rate(model_cache_misses_total{model=\"mrms\"}[$__rate_interval]))",
          "legendFormat": "GRIB Misses",
          "refId": "B"
        },
        {
          "expr": "sum(rate(model_grid_cache_hits_total{model=\"mrms\"}[$__rate_interval]))",
          "legendFormat": "Grid Hits",
          "refId": "C"
        },
        {
          "expr": "sum(rate(model_grid_cache_misses_total{model=\"mrms\"}[$__rate_interval]))",
          "legendFormat": "Grid Misses",
          "refId": "D"
        }
      ],
      "title": "MRMS Cache Hits vs Misses",
      "type": "timeseries"
    },
    {
      "datasource": {"type": "prometheus"},
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 20
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "red", "value": null},
              {"color": "yellow", "value": 50},
              {"color": "green", "value": 80}
            ]
