        }
      },
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 8,
        "y": 0
      },
      "id": 2,
      "targets": [
        {
          "expr": "(rate(tile_memory_cache_hits_total[1m]) + rate(cache_hits_total[1m])) / (rate(tile_memory_cache_hits_total[1m]) + rate(cache_hits_total[1m]) + rate(cache_misses_total[1m])) * 100",
          "legendFormat": "Combined (L1+L2)",
          "refId": "A"
        },
        {
          "expr": "rate(tile_memory_cache_hits_total[1m]) / (rate(tile_memory_cache_hits_total[1m]) + rate(tile_memory_cache_misses_total[1m])) * 100",
          "legendFormat": "L1 Only",
          "refId": "B"
        },
        {
          "expr": "rate(cache_hits_total[1m]) / (rate(cache_hits_total[1m]) + rate(cache_misses_total[1m])) * 100",
          "legendFormat": "L2 Only (Redis)",
          "refId": "C"
        }
      ],
      "title": "Cache Hit Rate (L1+L2 Combined)",
      "type": "timeseries"
    },
