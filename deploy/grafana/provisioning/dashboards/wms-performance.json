{
  "uid": "wms-perf",
  "title": "WMS Performance - Complete Pipeline Analysis",
  "tags": [
    "wms",
    "performance",
    "profiling"
  ],
  "timezone": "browser",
  "refresh": "5s",
  "schemaVersion": 40,
  "version": 4,
