{
  "uid": "goes-pipeline",
  "title": "GOES Satellite Pipeline - Complete Analysis",
  "tags": [
    "goes",
    "satellite",
    "netcdf",
    "performance"
  ],
  "timezone": "browser",
  "refresh": "5s",
  "schemaVersion": 40,
  "version": 1,
  "annotations": {
    "list": []
  },
  "templating": {
    "list": [
      {
        "name": "satellite",
        "type": "custom",
        "current": {
          "text": "All",
          "value": "$__all"
        },
        "options": [
          {"text": "All", "value": "$__all"},
          {"text": "GOES-16 (East)", "value": "goes16"},
          {"text": "GOES-18 (West)", "value": "goes18"}
        ],
        "includeAll": true,
        "multi": false
      }
    ]
  },
  "panels": [
    {
      "type": "row",
      "title": "Overview - GOES Request & Throughput",
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 0},
      "collapsed": false
    },
    {
      "datasource": {"type": "prometheus"},
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 20,
            "gradientMode": "opacity"
          },
          "color": {"mode": "palette-classic"}
        }
      },
      "gridPos": {"h": 7, "w": 8, "x": 0, "y": 1},
      "id": 1,
      "targets": [
        {
          "expr": "sum(rate(goes_requests_total[$__rate_interval])) by (satellite)",
          "legendFormat": "{{satellite}}",
          "refId": "A"
        },
        {
          "expr": "sum(rate(goes_requests_total[$__rate_interval]))",
          "legendFormat": "Total",
          "refId": "B"
        }
      ],
      "title": "GOES Requests/sec",
      "type": "timeseries"
    },
    {
      "datasource": {"type": "prometheus"},
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 20
          }
        }
      },
      "gridPos": {"h": 7, "w": 8, "x": 8, "y": 1},
      "id": 2,
      "targets": [
        {
          "expr": "sum(rate(goes_renders_total{success=\"true\"}[$__rate_interval])) by (satellite)",
          "legendFormat": "{{satellite}} success",
          "refId": "A"
        },
        {
          "expr": "sum(rate(goes_renders_total{success=\"false\"}[$__rate_interval])) by (satellite)",
          "legendFormat": "{{satellite}} failed",
          "refId": "B"
        }
      ],
      "title": "GOES Renders/sec (Success vs Failed)",
      "type": "timeseries"
    },
    {
      "datasource": {"type": "prometheus"},
      "fieldConfig": {
        "defaults": {
          "unit": "Bps",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 30,
            "gradientMode": "opacity"
          }
        }
      },
      "gridPos": {"h": 7, "w": 8, "x": 16, "y": 1},
      "id": 3,
      "targets": [
        {
          "expr": "sum(rate(goes_fetch_bytes_total[$__rate_interval])) by (satellite)",
          "legendFormat": "{{satellite}}",
          "refId": "A"
        }
      ],
      "title": "GOES Data Throughput (bytes/sec)",
      "type": "timeseries"
    },
    {
      "type": "row",
      "title": "Cache Performance",
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 8},
      "collapsed": false
    },
    {
      "datasource": {"type": "prometheus"},
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 20
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "red", "value": null},
              {"color": "yellow", "value": 50},
              {"color": "green", "value": 80}
            ]
          }
        }
      },
      "gridPos": {"h": 7, "w": 8, "x": 0, "y": 9},
      "id": 4,
      "targets": [
        {
          "expr": "sum(rate(goes_cache_hits_total[$__rate_interval])) by (satellite) / (sum(rate(goes_cache_hits_total[$__rate_interval])) by (satellite) + sum(rate(goes_cache_misses_total[$__rate_interval])) by (satellite)) * 100",
          "legendFormat": "{{satellite}}",
          "refId": "A"
        },
        {
          "expr": "sum(rate(goes_cache_hits_total[$__rate_interval])) / (sum(rate(goes_cache_hits_total[$__rate_interval])) + sum(rate(goes_cache_misses_total[$__rate_interval]))) * 100",
          "legendFormat": "Combined",
          "refId": "B"
        }
      ],
      "title": "GOES Grid Cache Hit Rate",
      "type": "timeseries"
    },
    {
      "datasource": {"type": "prometheus"},
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        }
      },
      "gridPos": {"h": 7, "w": 8, "x": 8, "y": 9},
      "id": 5,
      "targets": [
        {
          "expr": "sum(rate(goes_cache_hits_total[$__rate_interval])) by (satellite)",
          "legendFormat": "{{satellite}} hits",
          "refId": "A"
        },
        {
          "expr": "sum(rate(goes_cache_misses_total[$__rate_interval])) by (satellite)",
          "legendFormat": "{{satellite}} misses",
          "refId": "B"
        }
      ],
      "title": "GOES Cache Hits vs Misses",
      "type": "timeseries"
    },
    {
      "datasource": {"type": "prometheus"},
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 20
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "red", "value": null},
              {"color": "yellow", "value": 50},
              {"color": "green", "value": 80}
            ]
          }
        }
      },
      "gridPos": {"h": 7, "w": 8, "x": 16, "y": 9},
      "id": 6,
      "targets": [
        {
          "expr": "sum(rate(data_source_cache_hits_total{source=\"goes\"}[$__rate_interval])) / (sum(rate(data_source_cache_hits_total{source=\"goes\"}[$__rate_interval])) + sum(rate(data_source_parse_total{source=\"goes\"}[$__rate_interval]))) * 100",
          "legendFormat": "Parse Cache Hit Rate",
          "refId": "A"
        }
      ],
      "title": "GOES Data Source Cache Hit Rate",
      "type": "timeseries"
    },
    {
      "type": "row",
      "title": "Pipeline Latency Breakdown",
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 16},
      "collapsed": false
    },
    {
      "datasource": {"type": "prometheus"},
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 20,
            "stacking": {"mode": "normal"}
          }
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 17},
      "id": 7,
      "targets": [
        {
          "expr": "goes_fetch_duration_ms{quantile=\"0.5\"}",
          "legendFormat": "Fetch (p50)",
          "refId": "A"
        },
        {
          "expr": "goes_parse_duration_ms{quantile=\"0.5\"}",
          "legendFormat": "Parse (p50)",
          "refId": "B"
        },
        {
          "expr": "goes_projection_duration_ms{quantile=\"0.5\"}",
          "legendFormat": "Projection (p50)",
          "refId": "C"
        },
        {
          "expr": "png_encode_duration_ms{quantile=\"0.5\"}",
          "legendFormat": "PNG Encode (p50)",
          "refId": "D"
        }
      ],
      "title": "GOES Pipeline Stages - p50 Latency (Stacked)",
      "type": "timeseries"
    },
    {
      "datasource": {"type": "prometheus"},
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 17},
      "id": 8,
      "targets": [
        {
          "expr": "goes_render_duration_ms{quantile=\"0.5\"}",
          "legendFormat": "{{satellite}} p50",
          "refId": "A"
        },
        {
          "expr": "goes_render_duration_ms{quantile=\"0.95\"}",
          "legendFormat": "{{satellite}} p95",
          "refId": "B"
        },
        {
          "expr": "goes_render_duration_ms{quantile=\"0.99\"}",
          "legendFormat": "{{satellite}} p99",
          "refId": "C"
        }
      ],
      "title": "GOES Total Render Time (p50/p95/p99)",
      "type": "timeseries"
    },
    {
      "type": "row",
      "title": "Per-Stage Detailed Metrics",
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 25},
      "collapsed": false
    },
    {
      "datasource": {"type": "prometheus"},
